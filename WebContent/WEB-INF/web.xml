<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">

	<display-name>E-CIG | e-shop vape</display-name>

	<welcome-file-list>
		<welcome-file>e-cig/</welcome-file>
	</welcome-file-list>

		<context-param>
		<param-name>errorPage</param-name>
		<param-value>/WEB-INF/vue/erreur/errorPage.jsp</param-value>
	</context-param>
	
	  <error-page>
  	<error-code>404</error-code>
	<location>/vue/error/404.jsp</location>
  </error-page>
	
	  <error-page>
  	<exception-type>java.lang.Throwable</exception-type>
  	<location>/vue/error/pageErrorData.jsp</location>
  </error-page>


</web-app>